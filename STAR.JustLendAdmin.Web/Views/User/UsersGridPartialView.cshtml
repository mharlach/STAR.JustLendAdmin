@model List<User>

<p>
</p>

<div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th></th>
                <th>Email Address</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Enc ID</th>
                <th>Team</th>
                <th>Role</th>
                <th>Active</th>
            </tr>
        </thead>
        @for(var i = 0; i< Model.Count; i++)
        {
            <tr>
                <td><a asp-controller="User" asp-action="Edit" asp-route-id="@Model[i].Id">Edit</a></td>
                <td>@Model[i].Email</td>
                <td>@Model[i].FirstName</td>
                <td>@Model[i].LastName</td>
                <td>@Model[i].EncompassId</td>
                <td>@Model[i].TeamCode</td>
                <td>@Model[i].DetermineRole()</td>
                <td>@Model[i].Active</td>
            </tr>
        }
    </table>
</div>